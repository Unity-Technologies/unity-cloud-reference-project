/* Vector2Field */
.appui-vector2field {
    display: flex;
    flex-direction: column;
    width: 100%;
    flex-shrink: 0;
}

.appui-vector2field__container {
    display: flex;
    flex-direction: row;
}

.appui-vector2field__container > .appui-numericalfield {
    flex-basis: 50%;
    width: auto;
}

.appui-vector2field__x-field {
    margin-left: 0;
    margin-right: var(--appui-alias-control-gap-sm);
}

.appui-vector2field__y-field {
    margin-left: 0;
    margin-right: 0;
}
