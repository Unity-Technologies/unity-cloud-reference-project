.container__background-canvas {
    flex-grow: 1;
    background-color: rgb(0, 0, 0);
    -unity-background-scale-mode: stretch-to-fill;
    background-image: url('../Sprites/UnityBackground.png');
}

.image__logo {
    background-image: url('../Sprites/UnityLogo.png');
    position: absolute;
    width: 32px;
    height: 32px;
    top: 8px;
    left: 10px;
    flex: none;
    flex-grow: 0;
}
